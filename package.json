{
	"name": "v1",
	"version": "1.0.0",
	"scripts": {
		"devserver": "live-server",
		"prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/jb_modal.comp.css -o css/jb_modal.prefix.css",
		"compile:sass": "node-sass scss/jb_modal.scss css/jb_modal.comp.css",
		"compress:css": "node-sass css/jb_modal.prefix.css css/jb_modal.css --output-style compressed",
		"compress:js": "uglifyjs js/jb_modal.js -o js/jb_modal.min.js -m",
		"watch": "node-sass scss/jb_modal.scss css/jb_modal.css -w",
		"start": "npm-run-all --parallel devserver watch",
		"build": "npm-run-all compile:scss prefix:css compress:css compress:js"
	},
	"author": "Justin Bard",
	"license": "GNU3",
	"devDependencies": {
		"live-server": "^1.2.1",
		"node-sass": "^7.0.1",
		"npm-run-all": "^4.1.5",
		"uglify-js": "^3.14.5"
	}
}
